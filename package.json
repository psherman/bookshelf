{
  "name": "bookshelf",
  "version": "0.13.1",
  "description": "track books that you've read (or will read)",
  "main": "server/index.js",
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" public/css/index.css --dir public/css/",
    "build-dev": "npm run start & npm run css",
    "build-prod": "npm run build & npm run css",
    "css": "npm run sass & npm run autoprefix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass sass/** -o public/css/**",
    "start": "node devServer.js",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psherman/bookshelf.git"
  },
  "author": "Paul Sherman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psherman/bookshelf/issues"
  },
  "homepage": "https://github.com/psherman/bookshelf#readme",
  "dependencies": {
    "d3-scale": "^0.4.1",
    "history": "^1.17.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "node-sass": "^3.8.0",
    "postcss-cli": "^2.5.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  }
}
