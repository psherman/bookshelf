{
  "name": "bookshelf",
  "version": "0.12.3",
  "description": "track books that you've read (or will read)",
  "main": "server/index.js",
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" public/css/index.css --dir public/css/",
    "build-dev": "npm run webpack-dev & npm run css",
    "build-prod": "npm run webpack-prod & npm run css",
    "css": "npm run sass & npm run autoprefix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass sass/** -o public/css/**",
    "webpack-dev": "webpack",
    "webpack-prod": "webpack --config=webpack.config.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psherman/bookshelf.git"
  },
  "author": "Paul Sherman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psherman/bookshelf/issues"
  },
  "homepage": "https://github.com/psherman/bookshelf#readme",
  "dependencies": {
    "d3-scale": "^0.4.1",
    "history": "^1.17.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.3.3",
    "webpack": "^1.12.9"
  },
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  }
}
